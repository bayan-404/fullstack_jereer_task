{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "server/dist/js",
      "sourceMap": true,
      "rootDir": "server/src",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["server/src/**/*"],
    "exclude": ["node_modules", ".vscode"]
